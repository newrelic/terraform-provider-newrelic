{
    "ignorePatterns": [
        {
            "pattern": "^http(s)?://(?!(github.com|(www\\.)?terraform\\.io))"
        }
    ],
    "replacementPatterns": [
        {
            "pattern": "^(/docs/(?!providers/newrelic/))",
            "replacement": "https://terraform.io$1"
        },
        {
            "pattern": "^(?!http(s)?://)(.*)\\.html(#.*)?$",
            "replacement": "$2.html.markdown$3"
        }
    ]
}
