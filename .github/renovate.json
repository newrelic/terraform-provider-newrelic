{
  "baseBranches": [
    "master",
    "release/v1.x"
  ],
  "extends": [
    "config:base"
  ],
  "ignoreDeps": [
    "golang.org/x/crypto",
    "golang.org/x/net",
    "golang.org/x/sys",
    "google.golang.org/genproto"
  ],
  "commitMessagePrefix": "chore(deps):",
  "reviewers": [
    "team:developer-toolkit"
  ],
  "labels": [
    "automated pr",
    "dependencies"
  ],
  "postUpdateOptions": [
    "gomodTidy"
  ],
  "prConcurrentLimit": 4
}
